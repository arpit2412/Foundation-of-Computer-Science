import java.util.Scanner;
class Day{
	public static void main(String[] args) {
		int day;
		String dayn ;
		Scanner input = new Scanner(System.in);
		System.out.println("Enter the day number of the week");
		day = input.nextInt();
		switch (day) {
			case 1:dayn = "Monday";
				break;
			case 2:dayn = "Tuesday";
				break;
			case 3:dayn = "Wednesday";
				break;
			case 4:dayn = "Thursday";
				break;
			case 5:dayn = "Friday";
				break;
			case 6:dayn = "Saturday";
				break;
			case 7:dayn = "Sunday";
				break;
			default:System.out.println("Wrong Choice");
				dayn = "";
				break;
		}
		System.out.println(dayn);
	}	
}