class Problem03{
	public static void main(String[] args) {
		for(int i =1;i<41;i++){
			if(i<10){
				for(int j=0;j<i;j++){
					System.out.print("*");
				}
				System.out.println();
			}else if(i<20){
				for(int j=0;j<i;j++){
					System.out.print("=");
				}
				System.out.println();
			}else if(i<30){
				for(int j=0;j<i;j++){
					System.out.print("x");
				}
				System.out.println();
			}else{
				for(int j=0;j<i;j++){
					System.out.print("o");
				}
				System.out.println();
			}
		}
	}
	
}