//Class Calculator
public class Calculator{

	//Sum function 
	public int sum(int numA, int numB){
		return (numA + numB);
	}
	//Sub function
	public int sub(int numA, int numB){
		return (numA - numB);
	}
	//Multiply function
	public float multiply(float numA, float numB){
		return (numA * numB);
	}
	//Division function
	public float division(float numA, float numB){
		//Divide by 0 constraint
		if(numB != 0){
			return (numA / numB);
		}else{
			return (-99.0f);			//if dividend zero ptint -99.0
		}
	}
}