class ProblemAbstract {
	public static void solve(char [] arr, int n){
		int a = n;
		
		if(n==a){ 
			System.out.print("{");
			for(int i=0;i<=a;i++){
				System.out.print(arr[i]);
			}
			System.out.println("}");
		}else{
			System.out.print("{");
			for(int i=0;i<=a;i++){
				System.out.print(arr[i]);
			}
			System.out.print("}");
			System.out.print("{");
			for(int j=a;j<=n;j++){
				System.out.print(arr[j]);
			}
			System.out.println("}");
		}	

		if(a>0)
		solve(arr,a-1);			//recursive

		
		System.out.println();
	}
}