class ProblemAbstract {
	public static void solve(char [] arr, int n){
		System.out.print("{");
		for(int i=0;i<=n;i++){
			System.out.print(arr[i]);
		}
		System.out.println("}");
		if(n>0){
			solve(arr,n-1);			//recursive
		}
		
		System.out.println();
	}
}